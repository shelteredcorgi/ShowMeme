{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default permissions for ShowMeme",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-create",
    "core:window:allow-close",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-stat",
    "fs:allow-exists",
    "fs:allow-watch",
    "opener:default"
  ]
}
